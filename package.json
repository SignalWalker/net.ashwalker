{
  "name": "ashwalker-net",
  "version": "2.0.0",
  "description": "Ash Walker's personal website",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "eleventy --input=src --config=src/eleventy.config.mjs",
    "build-neocities": "env ASHWALKER_NET_NEOCITIES=1 eleventy --input=src --config=src/eleventy.config.mjs",
    "serve": "rm -r _site; eleventy --input=src --config=src/eleventy.config.mjs --serve",
    "serve-neocities": "rm -r _site; env ASHWALKER_NET_NEOCITIES=1 eleventy --input=src --config=src/eleventy.config.mjs --serve"
  },
  "repository": {
    "type": "git",
    "url": "https://git.ashwalker.net/ashwalker.net"
  },
  "author": "Ash Walker",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^5.0.0",
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "lightningcss": "^1.30.1",
    "markdown-it": "^14.1.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-footnote": "^4.0.0"
  }
}
