---
layout: 'layout/meta.njk'
mainClass: []
---

<header class="sidebar">
    <div class="window-border">
        <div class="window">
            {% include 'nav.njk'%}
        </div>
    </div>
</header>
<div id="main-display">
    <main class="{{ mainClass | join(" ") }}">
        {{ content | safe }}
    </main>
</div>
<footer class="sidebar">
    <div class="window-border">
        <div class="window">
            <div>
            {% include 'h-card.njk' %}
            </div>
        </div>
    </div>
    {%- if neocities -%}
    <div class="window-border">
        <div class="window">
            {% include 'buttons.njk' %}
        </div>
    </div>
    {%- endif -%}
</footer>
