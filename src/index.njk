---
title: About
layout: layout/main.njk
mainClass:
  - "center"
---

<section class="post-list">
    <h1>Recent Articles</h1>
    <ol role="list">
        {%- for post in collections.article | reverse -%}
        <li><a href="{{ post.url }}">{{ post.data.title }}</a>{{ post.date | toTimeTag | safe }}</li>
        {%- endfor -%}
    </ol>
</section>
<section class="project-gallery">
    <h1>Projects</h1>
    <ul role="list">
        {%- for project in collections.project -%}
        <li>{{ project.data.title }}</li>
        {%- endfor -%}
    </ul>
</section>
